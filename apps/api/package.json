{
  "name": "api",
  "private": true,
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "start:dev": "DATABASE_URL=file:./dev.db prisma db push --force-reset && DATABASE_URL=file:./dev.db prisma db seed && ts-node src/main.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "pretest": "DATABASE_URL=file:./test.db prisma db push && DATABASE_URL=file:./test.db prisma db seed",
    "test": "jest",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:db-push": "prisma db push",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jsonwebtoken": "9.0.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/testing": "^10.3.10",
    "@prisma/client": "^6.16.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "^22.8.6",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "^2.0.16",
    "eslint": "8.57.1",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  }
}
